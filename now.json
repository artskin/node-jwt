{
  "version": 2,
  "builds": [
    { "src": "public/", "use": "@now/static" },
    { "src": "public/app/index.js", "use": "@now/node-server" }
  ],
  "routes": [
    {
      "src": "/users",
      "dest": "/index.js",
      "methods": ["GET"]
    },
    {
      "src": "/userinfo",
      "dest": "public/app/index.js",
      "methods": ["GET"]
    }
  ]
}