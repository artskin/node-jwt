{
  "name": "now-express",
  "version": 2,
  "builds": [
		{ 
      "src": "/src/index.js",
      "use": "@now/node-server" 
    }
	],
	"routes": [
    {
      "src": "/(.*)", 
      "dest": "/src/index.js"
    },
    {
      "src": "/api/login", 
      "dest": "/src/index.js",
      "methods": ["POST"]
    },
    {
      "src": "/api/reg", 
      "dest": "/src/index.js",
      "methods": ["POST"]
    },
    {
      "src": "/api/userinfo", 
      "dest": "/src/index.js",
      "methods": ["GET"]
    }
  ]
}
